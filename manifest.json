{
    "manifest_version": 2,
  
    "name": "Time to Stop",
    "description": "A timer",
    "version": "1.0",
  
    "browser_action": {
      "default_icon": "icon.png",
      "default_title": "Set a timer!",
      "default_popup": "popup.html"
    },
    
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "web_accessible_resources": [
      "media/*"
    ]
  }